{
  "manifest_version": 2,

  "name": "Platzi Notificaciones",

  "description": "Esta extensión te ayuda a recordar eventos de la mejor plataforma de educación online",
  "version": "2.0",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },
  "background":{
       "page": "background.html"
  },
  "permissions":[
    "notifications"
  ],
  "icons": { 
    "16": "images/icon.png",
    "128": "images/icon128.png" 
  }
}
